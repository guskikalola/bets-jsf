<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>facade</managed-bean-name>
  <managed-bean-class>bean.FacadeBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>queryQuestion</managed-bean-name>
  <managed-bean-class>bean.QueryQuestionBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>createQuestion</managed-bean-name>
  <managed-bean-class>bean.CreateQuestionBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>login</managed-bean-name>
  <managed-bean-class>bean.LoginRegisterBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>diruaSartu</managed-bean-name>
  <managed-bean-class>bean.DiruaSartuBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>users</managed-bean-name>
  <managed-bean-class>bean.ManageUsersBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>logs</managed-bean-name>
  <managed-bean-class>bean.LogsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>azterketa</managed-bean-name>
 	<managed-bean-class>bean.AzterketaBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>ToCreateQuestion</display-name>
  <from-view-id>/Main.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>createQuestion</from-outcome>
   <to-view-id>/CreateQuestion.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>ToQueryQuestion</display-name>
  <from-view-id>/Main.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>queryQuestion</from-outcome>
   <to-view-id>/QueryQuestion.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>true</from-outcome>
   <to-view-id>/Main.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Register.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>true</from-outcome>
   <to-view-id>/Main.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/WEB-INF/facelets/templates/Goiburukoa.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/Main.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Erabiltzaileak.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>true</from-outcome>
   <to-view-id>/Erabiltzaileak.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
